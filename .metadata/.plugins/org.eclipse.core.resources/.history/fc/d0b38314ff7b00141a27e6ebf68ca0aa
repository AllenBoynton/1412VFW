// global variables
var beachNames = ["Miami South Beach, FL", "Myrtle Beach, SC", "Daytona Beach, FL", 
				"Laguna Beach, CA", "Venice Beach, CA", "Clearwater Beach, FL", 
				"Long Beach, NJ" ];

var i = 0;

var previousBeach = function(){
	if(i>0){
		titleText.text = beachNames[6];
		i--;
	} else {
		titleText.text = beachNames[i];
		i--;
	}
};

var nextBeach = function(){
	if(i<0){
		titleText.text = beachNames[6];
		i++;
	} else {
		titleText.text = beachNames[i];
		i++;
	}
};

previousButton.addEventListener("click", previousBeach);
nextButton.addEventListener("click", nextBeach);